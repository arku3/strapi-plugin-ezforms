{
  "name": "strapi-plugin-ezforms",
  "version": "1.0.0",
  "description": "Easy way to integrate forms into your Strapi app",
  "repository": {
    "type": "git",
    "url": "https://github.com/excl-networks/strapi-plugin-ezforms"
  },
  "license": "MIT",
  "author": {
    "name": "Excl Networks Inc."
  },
  "maintainers": [
    {
      "name": "Excl Networks Inc."
    }
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:serve": "vitepress serve docs",
    "format": "prettier --write \"server/src/**/*.{ts,js}\"",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "test": "jest --coverage",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "verify": "strapi-plugin verify",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.1",
    "@strapi/types": "^5.1.1",
    "@strapi/typescript-utils": "^5.1.1",
    "@types/jest": "^28.1.8",
    "@types/koa": "^2.15.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "jest-mock-axios": "^4.6.1",
    "typescript": "^5.6.3",
    "vitepress": "^1.0.0-alpha.15",
    "vue": "^3.2.39"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.1"
  },
  "engines": {
    "node": ">=18.x.x <=20.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "name": "ezforms",
    "description": "Easy way to integrate forms into your Strapi app",
    "kind": "plugin"
  }
}
